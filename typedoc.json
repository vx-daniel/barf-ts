{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs/api",
  "tsconfig": "./tsconfig.typedoc.json",
  "excludePrivate": true,
  "excludeInternal": true,
  "readme": "./docs/architecture.md",
  "validation": {
    "notExported": true,
    "invalidLink": true
  },
  "plugin": [
    "typedoc-plugin-zod",
    "typedoc-plugin-mdn-links",
    "typedoc-plugin-coverage"
  ],
  "navigationLinks": {
    "Architecture": "../architecture.md",
    "Guides": "../guides/issue-lifecycle.md",
    "GitHub": "https://github.com/your-org/barf-ts"
  },
  "highlightLanguages": ["bash", "typescript", "javascript", "mermaid"],
  "resolveUtilityTypes": true,
  "externalSymbolLinkMappings": {
    "@anthropic-ai/claude-agent-sdk": { "Query": "#" },
    "neverthrow": { "ResultAsync": "#" }
  }
}
